{
  "debug": false,
  "address": "ws://example.com:8080",
  "authToken": "",
  "retryTimes": 0,
  "waitingSeconds": 30,
  "admin": 0,
  "updateUrl": "https://github.com/Magic-Xin/MacArthurGo/releases/download/Prerelease-dev/",
  "updateInterval": 86400,
  "bannedList": [],
  "plugins": {
    "corpus": {
      "enable": false,
      "rules": [
        {
          "regexp": "^a$",
          "reply": "reply all",
          "scene": "all",
          "users": [123456],
          "groups": [123456, 654321]
        },
        {
          "regexp": "^g$",
          "reply": "reply group",
          "scene": "group"
        },
        {
          "regexp": "^p$",
          "reply": "reply private",
          "isReply": true,
          "isAt": false,
          "scene": "private"
        },
      ]
    },
    "originPic": {
      "enable": false,
        "args": ["/origin", "/o", "/原图", "/原"]
    },
    "repeat": {
      "enable": false,
      "times": 3,
      "probability": 0.4,
      "commonProbability": 0.001
    },
    "bili": {
      "enable": false,
      "aiSummarize": {
        "enable": true,
        "args": ["/summarize", "/sum"],
        "groupForward": true
      }
    },
    "poke": {
      "enable": false,
      "args": ["/poke", "/p"]
    },
    "roll": {
      "enable": false,
      "args": ["/roll", "/r"]
    },
    "music": {
      "enable": false
    },
    "picSearch":{
      "enable": false,
      "args": ["/search", "/s"],
      "groupForward": true,
      "allowPrivate": true,
      "handleBannedHosts": true,
      "expirationTime": 86400,
      "intervalTime": 1800,
      "searchFeedback": "",
      "sauceNAOToken": ""
    },
    "chatAI": {
      "enable": false,
      "chatGPT": {
        "enable": true,
        "args": ["/chatgpt", "/c"],
        "model": "gpt-3.5-turbo-1106",
        "apiKey": ""
      },
      "qWen": {
        "enable": true,
        "args": ["/qwen", "/q"],
        "model": "qwen-max",
        "apiKey": ""
      },
      "gemini": {
        "enable": true,
        "args": ["/gemini", "/g"],
        "apiKey": ""
      },
      "newBing": {
        "enable": true,
        "args": ["/bing", "/b"],
        "model": "Creative-g4t",
      },
      "groupForward": true,
      "panGu": true
    }
  }
}